{
  "name": "@frank17008/image-annotation",
  "version": "0.1.0",
  "description": "A modern React image annotation component with beautiful UI and SVG icons",
  "keywords": [
    "react",
    "image annotation component",
    "canvas",
    "typescript"
  ],
  "author": "FRANK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frank17008/image-annotation.git"
  },
  "homepage": "https://frank17008.github.io/image-annotation",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --env.NODE_ENV production",
    "dev": "tsup --env.NODE_ENV development --watch",
    "prepublishOnly": "pnpm run build",
    "preview": "pnpm run build && pnpm pack"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
